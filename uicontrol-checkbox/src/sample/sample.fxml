<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.CheckBox?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
<!--    checkbox bisa memiiliki banyak pilihan-->
<!--    membuat checkbox harus ada vbox yang berfungsi sebagai container-->
    <VBox>
        <CheckBox fx:id="ktp" text="Ada Ktp" textAlignment="JUSTIFY" textFill="orange"></CheckBox>
        <CheckBox fx:id="kk" text="Ada KK" textAlignment="JUSTIFY" textFill="orange"></CheckBox>
        <CheckBox fx:id="sim" text="Ada sim" textAlignment="JUSTIFY" textFill="orange"></CheckBox>
    </VBox>
</GridPane>